{
  "name": "vite-plugin-kintone-dev",
  "version": "1.4.0",
  "description": "vite plugin for developement kintone",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "pnpm unbuild",
    "release": "pnpm build && pnpm bumpp",
    "dev": "pnpm -r --filter=vue-dev-demo dev",
    "example:install": "pnpm -r install",
    "example:build": "pnpm -r --filter=vue-dev-demo build"
  },
  "homepage": "https://github.com/GuSanle/vite-plugin-kintone-dev",
  "repository": "https://github.com/GuSanle/vite-plugin-kintone-dev.git",
  "keywords": [
    "vite",
    "vite-plugin",
    "kintone"
  ],
  "author": "GuSanle",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.24",
    "bumpp": "^9.3.0",
    "rollup": "^4.12.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^6.1.2"
  },
  "dependencies": {
    "@inquirer/prompts": "^4.0.0",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "vite"
      ]
    }
  }
}
